$(document)
  .on('shopify:block:select', function(e){

    var blockId = e.detail.blockId;
    var $parentSection = $('#shopify-section-' + e.detail.sectionId);

    if ($parentSection.hasClass('featured-products-section')){
      productPage.initializeQuantityBox();
      productPage.init();
    }

    utils.resizeActionButtons();

});

$(document)
  .on('shopify:section:reorder', function(e){

    utils.pageBannerCheck();

});

$(document)
  .on('shopify:section:load', function(e){

    var $parentSection = $('#shopify-section-' + e.detail.sectionId);

    utils.pageBannerCheck();

    if ($parentSection.hasClass('faq-section')){
      faqAccordion.init();
    }

    if ($parentSection.hasClass('featured-products-section')){
      productPage.initializeQuantityBox();
      productPage.init();
    }

    if ($parentSection.hasClass('featured-collection-section')){
      featuredCollectionSection.init();
    }

    if ($parentSection.hasClass('product-template')){
      productPage.init();
    }

    if ($parentSection.hasClass('article-template-section')){
      if(window.location.pathname.indexOf('/comments') != -1) {
        $('html,body').animate({scrollTop: $("#new-comment").offset().top-140},'slow');
      }
    }

    if ($parentSection.hasClass('search-template-section')){
      collection.init();
    }

    if ($parentSection.hasClass('page-details-section')) {
      featuredCollectionSection.init();
    }

    if ($parentSection.hasClass('product-details-section')) {
      featuredCollectionSection.init();
    }
});

$(document)
  .on('shopify:section:unload', function(e){

    var $target = $(e.target);
    var $parentSection = $('#shopify-section-' + e.detail.sectionId);

    if ($parentSection.hasClass('product-template')){
      productPage.unload($target);
    }
});


/* CONSPIRE CUSTOM CODE */
$('.conspire-drawer-header').click(function(e) {
  	e.preventDefault();

    var $this = $(this);

    if ($this.next('.conspire-drawer').hasClass('show')) {
        $this.next('.conspire-drawer').removeClass('show');
        $this.next('.conspire-drawer').slideUp(300);
    } else {
        $this.next('.conspire-drawer').toggleClass('show');
        $this.next('.conspire-drawer').slideToggle(300);
    }
});

$('.conspire-drawer-header.active').click();

/* RTD Promo */
$(document).ready(function() {
  if (window.location.pathname.toLowerCase() === '/pages/drink-vital') {

    $('.rtd-slider').flickity({
      // options
      contain: true,
      lazyLoad: 1
    });
  }
});

/* RTD Official */
$(document).ready(function() {
    $('.rtd-slider.official').flickity({
      // options
      contain: true,
      lazyLoad: 1
    });
});


