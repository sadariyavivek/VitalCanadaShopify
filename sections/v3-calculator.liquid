  var minimumGramsWomen = 20;
  var lowRangeMultiplier = 0.6;
  
  $('.cc-form').on('submit', function(e) {
    e.preventDefault();
    
    var gramsPerDay = calculateGramsPerDay();
	var lowRangeGrams = calculateLowRange(gramsPerDay);
    var gramsRounded = roundToNearest10(gramsPerDay);
    var lowRangeGramsRounded = roundToNearest10(lowRangeGrams);
    var gramsClamped = clampMinimumGrams(gramsRounded);
    var lowRangeGramsClamped = clampMinimumGrams(lowRangeGramsRounded);
    
    if (gramsClamped === lowRangeGramsClamped) {
      gramsClamped += 10;
    }
    
    generateBundleData(Math.round(gramsPerDay), sex);
    
    $('.circle-number').text(lowRangeGramsClamped + "-" + gramsClamped);
    $('.cc-intro-page').addClass('inactive');
    $('.cc-results-page').removeClass('inactive');
    
    {% if page.handle contains "collagen-calculator" %}
    window.scrollTo(0,0);
    {% endif %}
    
    $('circle.gradient').addClass('active');
    
    if (gramsPerDay < 40) {
      $('circle.gradient').addClass('quarter');
    } else if (gramsPerDay < 60) {
      $('circle.gradient').addClass('half');
    } else if (gramsPerDay < 80) {
      $('circle.gradient').addClass('threeQuarters');
    } else {
      $('circle.gradient').addClass('full');
    }
    
    $('.bundle-cta').removeAttr('tabindex');
    
    {% if customer %}
    	{% assign customerHash = customer.id | hmac_sha256: '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' %}
        {% assign existingMetafields = '' %}
        {% for field in customer.metafields.calculator %}
            {% assign fieldKey = field | first %}
            {% assign fieldValue = "|" | append: fieldKey | append: "|" %}
            {% assign existingMetafields = existingMetafields | append: fieldValue %}
        {% endfor %}

        var formData = {
          "hash": "{{ customerHash }}",
          "customerID": "{{ customer.id }}",
          "gender": sex,
          "age": age,
          "weight": weight,
          "reason": reason,
          "gramsPerDay": Math.round(gramsPerDay),
          "diet": diet,
          "existingMetafields": "{{ existingMetafields }}"
      	};
        
    	$.ajax({
          url: 'https://collagen-calculator.herokuapp.com/profile-submit',
          dataType: 'json',
          cache: false,
          type: 'POST',
          data: formData,
          success: function (data) {
            
          },
          error: function(){
            console.log("Something went wrong - user profile not updated");
          }
        });
