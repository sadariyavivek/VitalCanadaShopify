    }
  }
  
  function calculateHydrolosis() {
    if (diet === "high") {
      return 3.0;
    } else if (diet === "low") {
      return 1.5;
    } else {
      return 2.25;
    }
  }
  
  function calculateAgeBasedDegradation() {
    var baseCollagenTissueContent = 3728.7600; // Age 29 and under
    
    if (age <= 29) {
      return baseCollagenTissueContent
    } else {
      var baseAge = 29;
      var percentLostEachYear = 0.0136;
      var degradation = baseCollagenTissueContent - (baseCollagenTissueContent * (age - 29) * percentLostEachYear);

      return degradation;
    }
  }
  
  function calculateGramsPerDay() {
    // All Magic numbers taken from spreadsheet
    const baseProcollagenDegradation = 923.9 * weightScaleFactor;
    const glycineEquivalentFinal = 12.04 * weightScaleFactor;
    
    // From table 3
    var baseCollagenTissueContent = 3728.7600;
    var ageAndWeightScaleFactor = calculateAgeBasedDegradation() * weightScaleFactor * 0.003229;

    // From table 2
    const porphryins = 240.0;
    const purines = 206.0;
    var creatineSynth = (calcCreatineSynth(age, sex) * 75.0);
    var glutathion = (567.00 * weightScaleFactor);
    const bileSalts = 60.0;
    
    const synthInMetabolism = 3.0;
    var hydrolosisDietaryProteins = calculateHydrolosis();
    var synthMetabolites = -((porphryins + purines + creatineSynth + glutathion + bileSalts) / 1000.0);
    var synthCollagen = -ageAndWeightScaleFactor;
    const synthOtherProteins = -1.0;
    
    var peptideEquivalent = -(((synthInMetabolism + hydrolosisDietaryProteins + synthMetabolites + synthCollagen + synthOtherProteins) / 3.719) * 20);
	
    
    return peptideEquivalent;
  }
  
