<script>

  $(document).ready(function() {

    setTimeout(function() {
      $('[id^="subscription-widget-select"]').val(4).change();
    }, 200);

    $(document).on('change', '.current-roast-container .subscription-widget-select', function(e) {
      var quickSubProductID = $(this).data('product-id');
      var selectedFrequency = $(this).val();
      var originalVariant = $(this).data('original-variant-id');
      var subscriptionVariant = $(this).data('subscription-variant-id');
      if (selectedFrequency == 0) {
        $('.current-roast-container .price .money').text($(this).data('price'));
        $(".money-was").addClass("strike-it-hide");
        $(".money-was").removeClass("strike-it-show");
      } else {
        $('.current-roast-container .price .money').text($(this).data('discount-price'));
        $(".money-was").addClass("strike-it-show");
        $(".money-was").removeClass("strike-it-hide");
      }
    });

  });
</script>


