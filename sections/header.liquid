  <script>
    function startAnimations() {
      $("#vitalMessages li").hide();

      function loop() {
        var $li = $("#vitalMessages li:first-child").fadeIn(500, function () {

          setTimeout(
            function() 
            {

              $li.fadeOut(500, function () {
                $li.appendTo('#vitalMessages');;
                loop()
              })

            }, 4000);

        });
      }
      loop()
    }

    startAnimations();
  </script>
