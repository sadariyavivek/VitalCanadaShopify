<script>
  function myFunction() {
    var element = document.getElementById("mobileNavigation");

    if (element.classList) { 
      element.classList.toggle("displayMenu");
    } else {
      var classes = element.className.split(" ");
      var i = classes.indexOf("displayMenu");

      if (i >= 0) 
        classes.splice(i, 1);
      else 
        classes.push("displayMenu");
      element.className = classes.join(" "); 
    }

    var element = document.getElementById("backdrop");

    if (element.classList) { 
      element.classList.toggle("backdropOn");
    } else {
      var classes = element.className.split(" ");
      var i = classes.indexOf("backdropOn");

      if (i >= 0) 
        classes.splice(i, 1);
      else 
        classes.push("backdropOn");
      element.className = classes.join(" "); 
    }

  }


  function openSearch() {
    var element = document.getElementById("mobileSearchContainer");

    if (element.classList) { 
      element.classList.toggle("displaySearch");
    } else {
      var classes = element.className.split(" ");
      var i = classes.indexOf("displaySearch");

      if (i >= 0) 
        classes.splice(i, 1);
      else 
        classes.push("displaysearch");
      element.className = classes.join(" "); 
    }

  }

  function openDesktopSearch() {
    var element = document.getElementById("desktopSearchContainer");

    if (element.classList) { 
      element.classList.toggle("displaySearch");
    } else {
      var classes = element.className.split(" ");
      var i = classes.indexOf("displaySearch");

      if (i >= 0) 
        classes.splice(i, 1);
      else 
        classes.push("displaysearch");
      element.className = classes.join(" "); 
    }

  }

</script>

<script>
  $(document).ready(function() {
  //toggle the component with class sub-menu
  $(".parent.megamenu").click(function() {
    if ($('.sub-menu').is(':visible')) {
      $(".sub-menu").slideUp(300);
      $(".plusminus").text('+');
    }
    if ($(this).next(".sub-menu").is(':visible')) {
      $(this).next(".sub-menu").slideUp(300);
      $(this).children(".plusminus").text('+');
    } else {
      $(this).next(".sub-menu").slideDown(300);
      $(this).children(".plusminus").text('-');
    }
  });
});
</script> 

