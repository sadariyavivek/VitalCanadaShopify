  .circle-wrapper {
    width: 140px;
    height: 140px;
    display: block;
    margin-left: auto;
    margin-right: auto;
    position: relative;
  }

  .circle-container {
    position: absolute;
    transform: rotateZ(-90deg);
  }
  
  circle.background {
    fill: transparent;
    stroke: #c1c0c0;
    stroke-width: 4;
    opacity: 0.5;
  }
  
  circle.gradient {
      fill: transparent;
      stroke: url(#circle-gradient);
      stroke-width: 20;
      stroke-dasharray: 720;
      stroke-dashoffset: 720;
  }
  
  circle.gradient.active {
    fill: transparent;
    stroke: url(#circle-gradient);
    stroke-width: 20;
    stroke-dasharray: 360;
    stroke-dashoffset: 360;
  }
  
  circle.gradient.active.quarter {
    animation: rotateQuarter 1.5s ease-out forwards;
    animation-iteration-count: 1;
    animation-delay: 0.4s;
  }
  
  circle.gradient.active.half {
    animation: rotateHalf 1.5s ease-out forwards;
    animation-iteration-count: 1;
    animation-delay: 0.4s;
  }
  
  circle.gradient.active.threeQuarters {
    animation: rotateThreeQuarters 1.5s ease-out forwards;
    animation-iteration-count: 1;
    animation-delay: 0.4s;
  }
  
    circle.gradient.active.full {
    animation: rotateFull 1.5s ease-out forwards;
    animation-iteration-count: 1;
    animation-delay: 0.4s;
  }

  @keyframes rotateQuarter {
    to {
      stroke-dashoffset: 270;
    }
  }
  
  @keyframes rotateHalf {
    to {
      stroke-dashoffset: 180;
    }
  }
  
  @keyframes rotateThreeQuarters {
    to {
      stroke-dashoffset: 90;
    }
  }
  
    @keyframes rotateFull {
    to {
      stroke-dashoffset: 0;
    }
  }
  
