<script>
  
  $('select').on('change', function() {
    var itemID = $(this).attr('id');
    var selectedOption = $('#' + itemID + ' option:selected');
    var selectedPrice = selectedOption.attr('data-price');
    var compareAtPrice = selectedOption.attr('data-compare-price');
    
    if (compareAtPrice > selectedPrice) {
      $('#price-' + itemID).html('$' + (selectedPrice / 100) + ' <s>' + '$' + (compareAtPrice / 100) + '</s>');
    } else {
      $('#price-' + itemID).html('$' + (selectedPrice / 100));
    }
    
    $('#product-form-' + itemID + ' input[name="id"]').val(selectedOption.val());
    
    
  });

  $('.product-dropdown-section .conspire-drawer-header').click(function(e) {
    e.preventDefault();
    var arrow = $(this).find('.icon-down-arrow');
    
    if (arrow.hasClass('open')) {
      arrow.removeClass('open');
      $(this).attr('aria-expanded', 'false');
    } else {
      arrow.addClass('open');
      $(this).attr('aria-expanded', 'true');
    }
  });
</script>