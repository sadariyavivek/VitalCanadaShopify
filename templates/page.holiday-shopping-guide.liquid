<a name="pagecontent" id="pagecontent"></a>
<style>
  .content.main {
    display: none;
  }
  
  .hsg-wrapper .hero-image {
    background-size: cover;
    background-position: 50%;
    background-repeat: no-repeat;
    background-image: url({{ 'Holiday-Offerings_Header.gif' | file_img_url: '2000x' }});
    width: 100%;
    height: 300px;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  
  .hsg-wrapper .hero-image h1 {
    color: white;
    text-transform: uppercase;
    font-size: 72px;
  }
  
  .trending-section {
    max-width: 1200px;
    margin: 0 auto;
    margin-bottom: 40px;
  }
  
  .trending-header {
    font-size: 60px;
    color: #23b6e6;
    text-align: center;
  }
  
  .trending-grid {
    display: flex;
    flex-direction: row;
    align-items: flex-start;
    flex-wrap: wrap;
  }
  
  .trending-grid .trending-item {
    width: 33.33%;
    display: block;
    text-align: center;
    transition: .2s transform ease-out, .2s opacity ease-out;
  }
  
  .trending-grid .trending-item:hover .ti-image-container {
	opacity: 0.7;
    transform: scale(0.95) translateY(-10px);
  }
  
  .trending-grid .trending-item:hover .hsg-title {
    opacity: 0.7;
  }
  
  .trending-grid .trending-item h3 {
    text-transform: uppercase;
    letter-spacing: 1px !important;
    color: #e5666b;
    font-size: 36px;
  }
  
  .trending-grid .trending-item .ti-image-container {
    width: 80%;
    margin-left: auto;
    margin-right: auto;
    background-size: cover;
    background-position: 50%;
    background-repeat: no-repeat;
    height: 240px;
    border-radius: 20px;
    transition: .2s all ease-out;
  }
  
  .trending-grid .trending-item h3.hsg-title {
    margin-bottom: 0;
    margin-top: 20px;
  }
  
  .trending-grid .trending-item h3.hsg-price {
    margin-bottom: 0;
    margin-top: 0px;
    font-size: 28px;
    line-height: 32px;
    color: #6ad8d3;
    transition: .2s all ease-out;
  }
  
  .product_form {
    margin-bottom: 0;
    opacity: 0;
    transform: translateY(-20px);
    max-height: 0;
    transition: 0.2s all ease-out;
  }
  
  .trending-grid .trending-item:hover .product_form {
    max-height: 120px;
    opacity: 1;
    transform: translateY(0);
  }
  
  
  button.add_to_cart {
    height: initial;
    min-height: auto;
    min-width: auto;
    width: auto;
    font-family: 'kapraregular';
    text-transform: uppercase;
    letter-spacing: 1px;
    padding: 4px 20px;
    background-color: #e5666b;
    border: 0;
    margin-top: 10px;
  }
  
  button.add_to_cart:hover {
    background-color: #e5666b;
    color: white;
    opacity: 0.75;
    border: 0;
  }
  
  .new-products-section {
    max-width: 1200px;
    margin: 0 auto;
    margin-bottom: 40px;
  }
  
  .new-products-header {
    font-size: 60px;
    color: #23b6e6;
    text-align: center;
  }
  
  .new-products-grid {
    display: flex;
    flex-direction: row;
    align-items: flex-start;
    flex-wrap: wrap;
  }
  
  .new-products-item {
    width: 50%;
    display: block;
    text-align: center;
    transition: .2s transform ease-out, .2s opacity ease-out;
  }
  
  .new-products-item:hover {
    transform: translateY(-3px);
    opacity: 0.7;
  }
  
  .new-products-item h3 {
    text-transform: uppercase;
    letter-spacing: 1px !important;
    color: #e5666b;
    margin-top: 20px;
    font-size: 36px;
  }
  
  .new-products-item .np-image-container {
    width: 80%;
    margin-left: auto;
    margin-right: auto;
    background-size: cover;
    background-position: 50%;
    background-repeat: no-repeat;
    height: 240px;
    border-radius: 20px;
  }
  
  .mid-promo-banner {
    width: 100%;
  }
  
  .best-sellers-section {
    max-width: 1200px;
    margin: 0 auto;
  }
  
  .product-loop-container {
    max-width: 1200px;
    margin: 0 auto;
  }
  
  .best-sellers-header {
    font-size: 60px;
    color: #23b6e6;
    text-align: center;
  }
  
  .offerings-text {
    color: #23b6e6;
    letter-spacing: 1px !important;
    font-size: 36px;
    font-style: italic;
    text-align: center;
    text-transform: none;
  }
  
  .product-list .thumbnail {
    width: 25%;
  }
  
  .temp-placeholder {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
  }
  
  .temp-placeholder-half {
    width: 50%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding: 20px;
    border-radius: 10px;
    overflow: hidden;
  }
  
  .temp-placeholder-half img {
    border-radius: 10px;
    width: 100%;
  }
  
  .temp-placeholder-half h4 {
    letter-spacing: 1px !important;
    color: #e5666b;
    font-size: 36px;
    text-align: center;
    text-transform: uppercase;
  }
  
  .temp-placeholder-half h5 {
    letter-spacing: 1px !important;
    color: #4a4a4a;
    font-size: 28px;
    text-align: center;
  }
  
  .temp-placeholder-half a {
    letter-spacing: 1px !important;
    color: #23b6e6;
    font-size: 36px;
    text-align: center;
    font-family: 'kapraregular';
    text-transform: uppercase;
    padding: 10px;
    border: 2px solid #23b6e6;
    border-radius: 10px;
  }
  
  .temp-placeholder-half a:hover {
    background-color: #23b6e6;
    color: white;
    transform: translateY(-2px);
  }
  
  @media screen and (max-width: 800px) {
    .hsg-wrapper {
      margin-top: 120px;
    }
    
    .hsg-wrapper .hero-image h1 {
      font-size: 36px;
      line-height: 40px;
    }
    
    .trending-grid .trending-item {
      width: 100%;
      margin-bottom: 40px;
    }
    
    .new-products-item {
      width: 100%;
      margin-bottom: 40px;
    }
    
    .product-list .thumbnail {
      width: 50%;
    }
    
    .product_form {
      opacity: 1;
      transform: translateY(0);
      max-height: 200px;
    }
    
    .temp-placeholder-half {
      width: 100%;
      margin-bottom: 30px;
    }
  }
</style>
</div>
