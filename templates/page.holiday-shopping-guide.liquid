  <!--<img class="mid-promo-banner" src="{{ 'HOLIDAYOFFERINGS_Banner.jpg' | file_img_url: '2000x' }}"/>-->
  <div class="best-sellers-section">
    <h2 class="best-sellers-header">shop best sellers</h2>
